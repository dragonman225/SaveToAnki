{
  "name": "SaveToAnki",
  "description": "Save Google translate results to Anki via AnkiConnect.",
  "version": "0.3.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/config.js",
        "src/ui.js",
        "src/extension.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "src/config.js",
      "src/background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "http://127.0.0.1/"
  ]
}